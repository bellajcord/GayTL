<main class="edit-form-page">

    <a class="d-inline-block mt-2 mb-4" href="/healths">Return To Previous Page</a>

    <section class="edit-form">

        <h2>Edit {{healths.name}} </h2>




        <form action="/health/{{healths._id}}?_method=PUT" method="POST">

            Name: <input type="text" name="name" value="{{healths.name}}" /><br />
            <br />
            Description: <input type="text" name="description" value="{{healths.description}}" /><br />
            <br />
            Image: <input type="text" name="img" value="{{healths.location}}" /><br />
            <br />
            Location: <input type="text" name="location" value="{{healths.img}}" /><br />
            <br />
            <input class="btn btn-sucess" type="submit" value="Update" />
        </form>
    </section>
</main>