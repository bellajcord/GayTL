<main>

	<h2>You are viewing:

		{{ night.name }}

	</h2>

	<a href="/">Return to Home Page</a>

	<div class="night-container-show">
		<h2 id="night-name">

				{{ night.name }}

		</h2>

				<img src="{{ night.img }}"/>

		<br>
		<h3>Description:</h3>
			<p>

				{{ night.description }}

			</p><br>
		<h3>Location:</h3>
			<p id="location">$

				{{ night.location }} </p><br>

		</div>

		<form action="/{{ night.id }}?_method=DELETE" method="POST">
			<input type="submit" value="Delete Club"/>
		</form>
</main>